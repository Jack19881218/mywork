<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
        android:title="@string/app_name" >
            
    <PreferenceScreen
            android:title="@string/root_permission_name">
        <intent android:action="android.intent.action.MAIN"
                android:targetPackage="com.huamobile.huasecretcode"
                android:targetClass="com.huamobile.huasecretcode.HuaAdbRootActivity" />
    </PreferenceScreen>
        
    <PreferenceScreen
            android:title="@string/version_name">
        <intent android:action="android.intent.action.MAIN"
                android:targetPackage="com.huamobile.huasecretcode"
                android:targetClass="com.huamobile.huasecretcode.HuaVersionInfoActivity" />
    </PreferenceScreen>


    <PreferenceScreen
            android:title="@string/hardware_info_name">
        <intent android:action="android.intent.action.MAIN"
                android:targetPackage="com.huamobile.huasecretcode"
                android:targetClass="com.huamobile.huasecretcode.huahardwareinfo.HuaHardwareInfoActivity" />
    </PreferenceScreen>

	<PreferenceScreen
			android:title="@string/battery_info">
		<intent android:action="android.intent.action.MAIN"
				android:targetPackage="com.android.settings"
				android:targetClass="com.android.settings.BatteryInfo" />
	</PreferenceScreen>

	<!--PreferenceScreen
		android:title="@string/factory_test_tool">
				        <intent android:action="android.intent.action.MAIN"
						android:targetPackage="com.spreadst.validationtools"
						android:targetClass="com.spreadst.validationtools.VolidationToolsMenuActivity"/>
						</PreferenceScreen-->

	<PreferenceScreen
			android:title="@string/marvell_hiden_tool">
		<intent android:action="android.intent.action.MAIN"
			android:targetPackage="com.marvell.hidenicon"
			android:targetClass="com.marvell.hidenicon.HidenAppActivity" />
	</PreferenceScreen>



	<PreferenceScreen
			android:title="@string/usage_statistics">
		<intent android:action="android.intent.action.MAIN"
			android:targetPackage="com.android.settings"
			android:targetClass="com.android.settings.UsageStats" />
	</PreferenceScreen>

  </PreferenceScreen>
        
